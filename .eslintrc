{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true 
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "max-len": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "spaced-comment": "off",
        "comma-spacing": "off",
        "eol-last": "off",
        "require-jsdoc": "off"

    },
    "extends": ["eslint:recommended", "google"]
}